{
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/wp-admin",
      "destination": "/"
    },
    {
      "source": "/wp-admin/**",
      "destination": "/"
    },
    {
      "source": "/wp-content/**",
      "destination": "/"
    },
    {
      "source": "/wp-includes/**",
      "destination": "/"
    }
  ],
  "headers": [
    {
      "source": "**/*.@(jpg|jpeg|gif|png|svg|ico|webp)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "**/*.@(css|js)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
